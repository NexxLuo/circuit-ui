{
  "env": {
    "cjs": {
      "plugins": [
        "@babel/plugin-proposal-class-properties",
        "lodash",
      ],
      "presets": [
        ["@babel/preset-env", { "loose": true }],
        "@babel/preset-react",
        [
          "@emotion/babel-preset-css-prop",
          {
            "autoLabel": false,
            "labelFormat": "[filename]--[local]"
          }
        ]
      ]
    },
    "es": {
      "plugins": [
        "@babel/plugin-proposal-class-properties",
        "lodash",
      ],
      "presets": [
        ["@babel/preset-env", { "loose": true, "modules": false }],
        "@babel/preset-react",
        [
          "@emotion/babel-preset-css-prop",
          {
            "autoLabel": false,
            "labelFormat": "[filename]--[local]"
          }
        ]
      ]
    },
    "test": {
      "plugins": [
        "@babel/plugin-proposal-class-properties",
        "lodash",
      ],
      "presets": [
        "@babel/preset-env",
        "@babel/preset-react",
        [
          "@emotion/babel-preset-css-prop",
          {
            "autoLabel": false
          }
        ]
      ]
    },
  }
}
